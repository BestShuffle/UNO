<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="fr.uno.client.graphic.controller.LobbyFrameController" alignment="center" hgap="10"
          vgap="10">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label style="-fx-font: NORMAL 60 Tahoma;" text="UNO"
           GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>

    <Button text="Jouer" onAction="#handlePlayButton" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
</GridPane>